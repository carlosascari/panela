{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  },
  "opts": {
    "recurse": true,
    "encoding": "utf8",
    "destination": "./docs",
    "readme": "./README.md"
  }
}